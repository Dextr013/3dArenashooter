; Engine configuration file.
; It is best edited using the Godot editor.
; Format: https://docs.godotengine.org/en/latest/classes/class_projectsettings.html

config_version=5

[application]
config/name="3D Arena Shooter"
run/main_scene="res://scenes/Main.tscn"
config/features=PackedStringArray("4.4", "Vulkan Clustered")

[display]
window/size/viewport_width=1920
window/size/viewport_height=1080
window/stretch/mode="viewport"
window/stretch/aspect="expand"
window/stretch/shrink=1

[renderer]
rendering_method="forward_plus"
rendering_method.mobile="mobile"
scaling_3d/mode=3
scaling_3d/scale=1.0
scaling_3d/scale_threshold_fps=50.0
scaling_3d/minimum_scale=0.6

[input]
move_forward={"deadzone":0.1,"events":[{"type":"InputEventKey","keycode":87,"physical_keycode":87,"unicode":0,"echo":false,"pressed":true}]}
move_backward={"deadzone":0.1,"events":[{"type":"InputEventKey","keycode":83,"physical_keycode":83,"unicode":0,"echo":false,"pressed":true}]}
move_left={"deadzone":0.1,"events":[{"type":"InputEventKey","keycode":65,"physical_keycode":65,"unicode":0,"echo":false,"pressed":true}]}
move_right={"deadzone":0.1,"events":[{"type":"InputEventKey","keycode":68,"physical_keycode":68,"unicode":0,"echo":false,"pressed":true}]}
jump={"deadzone":0.1,"events":[{"type":"InputEventKey","keycode":32,"physical_keycode":32,"unicode":0,"echo":false,"pressed":true},{"type":"InputEventJoypadButton","device":-1,"button_index":0,"pressure":0.0,"pressed":true}]}
fire={"deadzone":0.1,"events":[{"type":"InputEventMouseButton","device":-1,"button_index":1,"cancelled":false,"pressed":true,"doubleclick":false,"position":{"x":0.0,"y":0.0},"global_position":{"x":0.0,"y":0.0},"factor":0.0}]}
aim={"deadzone":0.1,"events":[{"type":"InputEventMouseButton","device":-1,"button_index":2,"cancelled":false,"pressed":true,"doubleclick":false,"position":{"x":0.0,"y":0.0},"global_position":{"x":0.0,"y":0.0},"factor":0.0}]}
reload={"deadzone":0.1,"events":[{"type":"InputEventKey","keycode":82,"physical_keycode":82,"unicode":0,"echo":false,"pressed":true}]}
dash={"deadzone":0.1,"events":[{"type":"InputEventKey","keycode":16777237,"physical_keycode":0,"unicode":0,"echo":false,"pressed":true}]}
pause={"deadzone":0.1,"events":[{"type":"InputEventKey","keycode":16777217,"physical_keycode":0,"unicode":0,"echo":false,"pressed":true}]}
mobile_fire={"deadzone":0.0,"events":[]}
mobile_jump={"deadzone":0.0,"events":[]}
mobile_reload={"deadzone":0.0,"events":[]}

[rendering]
anti_aliasing/high_quality=true
environment/defaults/default_environment="res://scenes/default_env.tres"
textures/anisotropic_filter_level=4
lights/penumbra_size=0.2
global_illumination/gi/use_half_res=true

[physics]
common/enable_physics_jitter_fix=true

[audio]
bus/layout="res://audio/audio_bus_layout.tres"